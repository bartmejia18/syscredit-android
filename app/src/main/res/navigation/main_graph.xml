<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.syscredit.ui.MainFragment"
        tools:layout="@layout/fragment_main">
        <action
            android:id="@+id/action_mainFragment_to_detailCustomer"
            app:destination="@id/detailCustomer" />
    </fragment>

    <fragment
        android:id="@+id/detailCustomer"
        android:name="com.example.syscredit.ui.DetailCustomer"
        android:label="fragment_detail_customer"
        tools:layout="@layout/fragment_detail_customer" />
</navigation>